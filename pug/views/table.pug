extends ./layouts/index

block section


    table( class="table table-info table-striped ")
        thead
            tr
                th(scope="col") Nombre
                th(scope="col") Precio
                th(scope="col") Imagen
        tbody
            each product in completeList
                tr
                    td= (product.nombre)
                    td= (product.precio)
                    td
                        img(src= product.url width="100px" alt= product.name)
    button(onclick="window.location.href='/'",class="btn  btn-primary mb-2") Cargar productos


